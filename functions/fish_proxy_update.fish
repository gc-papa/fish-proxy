function fish_proxy_update
    echo "========================================"
    echo "fish-proxy Update"
    echo "========================================"
    echo "To update fish-proxy, run:"
    echo "fisher remove gc-papa/fish-proxy"
    echo "fisher install gc-papa/fish-proxy"
    echo ""
    echo "Or if you installed manually:"
    echo "cd ~/.config/fish/plugins/fish-proxy"
    echo "git pull origin main"
    echo "========================================"
end
